<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context="com.taotaohai.fragment.VideoFragment">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="match_parent"
        android:layout_height="@dimen/title"
        android:background="@color/them" />

    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="match_parent"
        android:layout_height="130px"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView5"
        android:background="@color/them"
        android:paddingTop="30px"
        tools:ignore="PxUsage,UselessParent">

        <TextView
            style="@style/text36"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="@dimen/gap"
            android:text="视频"
            tools:ignore="HardcodedText" />

        <LinearLayout
            android:gravity="right"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"

            >
            <RelativeLayout

                android:id="@+id/search_"
                android:layout_width="64px"
                android:layout_height="match_parent"
                android:layout_alignParentTop="true"
                android:layout_marginRight="@dimen/gap"
                android:layout_toStartOf="@+id/vrelativeLayout3"
                tools:ignore="RtlHardcoded,UselessParent">

                <ImageView

                    android:layout_width="44px"
                    android:layout_height="44px"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:src="@mipmap/search_"
                    tools:ignore="ContentDescription" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/vrelativeLayout3"
                android:layout_width="64px"
                android:layout_height="match_parent"
                android:layout_alignParentTop="true"
                android:layout_marginRight="@dimen/gap"
                android:layout_toStartOf="@+id/vrelativeLayout2"
                tools:ignore="RtlHardcoded,UselessParent">

                <ImageView
                    android:layout_width="44px"
                    android:layout_height="44px"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:src="@mipmap/message"
                    tools:ignore="ContentDescription" />
            </RelativeLayout>



            <RelativeLayout
                android:id="@+id/vrelativeLayout2"
                android:layout_width="64px"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/gap"
                tools:ignore="RtlHardcoded,UselessParent">

                <ImageView
                    android:id="@+id/v_car_image"
                    android:layout_width="44px"
                    android:layout_height="44px"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:src="@mipmap/shopcart"
                    tools:ignore="ContentDescription" />

            </RelativeLayout>
        </LinearLayout>








    </RelativeLayout>
    <com.taotaohai.myview.XListView
        android:id="@+id/xlistview"
        android:layout_below="@+id/relativeLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
    </com.taotaohai.myview.XListView>


